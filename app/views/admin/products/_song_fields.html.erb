<div class="nested-fields">
  <div class="row">
    <div class="col-2">
      <label>No.</label>
      <%= f .text_field :track_num, class: "form-control" %>
    </div>
    <div class="col-8">
      <label>曲名</label>
      <%= f.text_field :song, class: "form-control" %>
    </div>
    <div class="col-2" style="padding-top: 32px;">
      <%= link_to_remove_association "消去", f, class: "btn btn-danger" %>
    </div>
  </div>
  <br>
</div>
