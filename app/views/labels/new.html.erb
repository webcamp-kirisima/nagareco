<div class="container">
  <%= form_for(@label) do |f| %>
    <%= render "layouts/error_messages", model: f.object %>
    <div class="form-group">
      <%= f.label :"レーベル名" %>
      <%= f.text_field :label, class: "form-control" %>
    </div>
    <div>
      <%= f.submit '追加', class: "btn btn-success" %>
    </div>
  <% end %>
</div>
